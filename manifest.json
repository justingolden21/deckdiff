{
  "name": "DeckDIFF",
  "short_name": "DeckDIFF",
  "version": "0.9.5",
  "description": "Diffing for Hearthstone decks",
  "icons": {
    "128": "img/icon.png"
  },
  "manifest_version": 2,
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_popup": "ui.html"
  },
  "options_page": "options.html?maximized",
  "permissions": [
    "clipboardRead",
    "clipboardWrite",
    "storage",
    "tabs",
    "https://api.hearthstonejson.com/",
    "<all_urls>"
  ],
  "applications": {
    "gecko": {
      "id": "deckdiff@zealot.hu"
    }
  },
  "content_security_policy": "script-src 'self' https://cdnjs.cloudflare.com https://use.fontawesome.com https://unpkg.com https://maxcdn.bootstrapcdn.com https://code.jquery.com; object-src 'self'"
}